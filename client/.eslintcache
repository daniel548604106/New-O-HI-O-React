ToCart(state.cartItems, action.payload) };\n    case REMOVE_CART_ITEM:\n      console.log('remove');\n      saveCartItemsToLocalStorage({\n        ...state,\n        cartItems: removeItemFromCart(state.cartItems, action.payload),\n      });\n      return { ...state, cartItems: removeItemFromCart(state.cartItems, action.payload) };\n    case UPDATE_CART_ITEM_QTY:\n      console.log('update');\n      saveCartItemsToLocalStorage({\n        ...state,\n        cartItems: updateCartItemQuantity(state.cartItems, action.payload),\n      });\n      return { ...state, cartItems: updateCartItemQuantity(state.cartItems, action.payload) };\n    case CLEAR_CART_ITEM:\n      console.log('clear');\n      saveCartItemsToLocalStorage({\n        ...state,\n        cartItems: clearItemFromCart(state.cartItems, action.payload),\n      });\n      return { ...state, cartItems: clearItemFromCart(state.cartItems, action.payload) };\n    default:\n      return initialState;\n  }\n};\n","/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/store/cart/cartUtils.js",[],"/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/store/cart/cartAction.js",[],"/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/components/Cart/CartPaymentInfo/CartPaymentInfoCard/CartPaymentInfoCard.jsx",[],"/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/components/Cart/CartConfirmInfo/CartConfirmInfo.jsx",[],[],"/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/components/Cart/CartListInfo/CartProductCard/CartProductCard.jsx",[],"/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/components/Cart/CartListInfo/CartSummary/CartSummary.jsx",[],"/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/components/Cart/CartListInfo/CartListInfo.jsx",["543"],"import React from 'react';\nimport classes from './CartListInfo.module.scss';\nimport CartProductCard from './CartProductCard/CartProductCard.jsx';\nimport Empty from '../../Global/Empty/Empty.js