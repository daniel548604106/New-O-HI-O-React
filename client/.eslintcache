ography\n                  gutterBottom\n                  variant=\"h6\"\n                  component=\"h2\"\n                  style={{\n                    marginBottom: '0',\n                    textOverflow: 'ellipsis',\n                    width: '100%',\n                    whiteSpace: 'nowrap',\n                    overflow: 'hidden',\n                  }}\n                >\n                  {product.name}\n                </Typography>\n                <div style={{ display: 'flex', alignItems: 'flex-end' }}>\n                  <Typography\n                    variant=\"h6\"\n                    color=\"textSecondary\"\n                    component=\"p\"\n                    style={{\n                      fontWeight: 'bold',\n                      textDecoration: product.discountPrice ? 'line-through' : ' none',\n                      color: 'black',\n                      marginRight: '5px',\n                    }}\n                  >\n                    $ {product.fullPrice}\n                  </Typography>\n                  {product.discountPrice && (\n                    <Typography\n                      variant=\"h6\"\n                      color=\"textSecondary\"\n                      component=\"p\"\n                      style={{ fontSize: '16px', color: '#178fac' }}\n                    >\n                      $ {product.discountPrice}\n                    </Typography>\n                  )}\n                </div>\n              </CardContent>\n            </CardActionArea>\n          </Card>\n        ))}\n      </div>\n    </div>\n  );\n};\n\nSale.propTypes = {\n  products: PropTypes.Array,\n};\n\nexport default Sale;\n","/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/components/Home/Banner/Banner.js",[],"/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/components/Global/Tags/Tags.jsx",[],"/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/components/Latest/ProductList/ProductList.jsx",["322"],"