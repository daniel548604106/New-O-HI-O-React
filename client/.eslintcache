</Grid>\n    </div>\n  );\n};\n\nexport default Beauty;\n","/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/components/Navbar/Header.jsx",["688","689","690","691","692","693","694","695","696"],"/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/components/Footer/Footer.jsx",["697"],"import React, { useState } from 'react';\nimport LanguageIcon from '@material-ui/icons/Language';\nimport Logo from '../../assets/images/global/O.HI.O-footer.svg';\nimport { useTranslation } from 'react-i18next';\n\nimport classes from './Footer.module.scss';\nconst Footer = () => {\n  const { t, i18n } = useTranslation();\n  const [language, setLanguage] = useState('English');\n  const changeLanguage = () => {\n    language === 'English'\n      ? (setLanguage('繁體中文(台灣)'), i18n.changeLanguage('en'))\n      : (setLanguage('English'), i18n.changeLanguage('tw'));\n  };\n  return (\n    <div className={classes.footerLayout}>\n      <img src={Logo} alt=\"logo\" />\n      <p>Design the way you are.</p>\n      <div className={classes.langaugeSelector}>\n        <LanguageIcon />\n        <p onClick={() => changeLanguage()}>{language}</p>\n      </div>\n      <p className={classes.copyRight}>© {new Date().getFullYear()} O.HI.O. All Rights Reserved.</p>\n    </div>\n  );\n};\n\nexport default Footer;\n","/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/pages/index.js",[],[],"/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/store/rootReducer.js",[],"/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/api/index.js",[],"/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/components/Navbar/Dropdown/Dropdown.jsx",["698"],"import React from 'react';\nimport { Link } from 'react-router-dom';\nimport classes from './Dropdown.module.scss';\nimport { useDispatch, useSelector } from 'react-redux';\nimport { setUserLogout } from '../../../store/user/userAction';\nimport { useHistory } from 'react-router-dom';\nconst Dropdown = () =>