opInfo} />\n      <div className={classes.mainContent}>\n        <ShopInfo shop={shopInfo} />\n        <Tabs tabs={tabs} />\n\n        {location.search.includes('product') && (\n          <div className={classes.product}>\n            <RecommendedDesign pinnedProducts={shopInfo.pinnedProducts} />\n            <div className={classes.productsRow}>\n              <Sidebar />\n              <div className={classes.productList}>\n                <ProductList products={shopInfo.products} />\n              </div>\n            </div>\n          </div>\n        )}\n        {location.search.includes('story') && (\n          <div className={classes.story}>\n            <p>{shopInfo.story}</p>\n          </div>\n        )}\n        <div className={classes.policy}>\n          {location.search.includes('policy') && <RefundPolicy />}\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default Shop;\n","/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/components/Global/Tabs/Tabs.jsx",["764","765","766"],"import React, { useState, useEffect } from 'react';\nimport classes from './Tabs.module.scss';\nimport PropTypes from 'prop-types';\nimport { useLocation, useHistory } from 'react-router-dom';\nconst Tabs = ({ tabs }) => {\n  const location = useLocation();\n  const history = useHistory();\n  const [activeTab, setActiveTab] = useState(0);\n  const switchTab = (idx, tab) => {\n    history.push(`${location.pathname}?tab=${tab}`);\n  };\n  useEffect(() => {\n    console.log(location);\n  }, []);\n  return (\n    <div>\n      <div className={classes.list}>\n        {tabs.map((tab, idx) => (\n          <div\n            key={tab.name}\n            onClick={() => switchTab(idx, tab.location)}\n            className={location.search.includes(`${tab.location}`) && classes.active}\n          >\n            {tab.name}\n          </div>\n        ))}\n      </div>\n    </div>\n  );\n};\n\nTabs.propTypes = {\n  tabs: PropTypes.array,\n};\nexport default Tabs;\n","/Users