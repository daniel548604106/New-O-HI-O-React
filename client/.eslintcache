y}>\n          <select name=\"year\" id=\"year\">\n            {yearSelection.map((index) => (\n              <option key={index} value={index}>\n                {index}\n              </option>\n            ))}\n          </select>\n          <span> / </span>\n          <select name=\"month\" id=\"month\">\n            {monthSelection.map((index) => (\n              <option key={index} value={index}>\n                {index}\n              </option>\n            ))}\n          </select>\n          <span> / </span>\n          <select name=\"date\" id=\"date\">\n            {dateSelection.map((index) => (\n              <option key={index} value={index}>\n                {index}\n              </option>\n            ))}\n          </select>\n        </div>\n      </form>\n      <span onClick={() => save()}>\n        <Button text=\"save\" />\n      </span>\n      <hr className={classes.hr} />\n      <ChangeProfilePicture />\n      <LanguagePreference />\n    </div>\n  );\n};\n\nexport default General;\n","/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/components/My/Content/Setting/General/ChangeProfilePicture/ChangeProflePicture.jsx",[],"/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/components/My/Content/Setting/General/LanguagePreference/LanguagePreference.jsx",[],"/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/components/Global/Empty/Empty.jsx",[],"/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/pages/Shop/Shop.jsx",["622","623"],"import React, { useEffect, useState } from 'react';\nimport classes from './Shop.module.scss';\nimport Banner from '../../components/Shop/Banner/Banner.jsx';\nimport ShopInfo from '../../components/Shop/ShopInfo/ShopInfo.jsx';\nimport Tabs from '../../components/Global/Tabs/Tabs.jsx';\nimport RecommendedDesign from '../../components/Shop/RecommendedDesign/RecommendedDesign.jsx';\nimport ProductList from '../../components/Shop/ProductList/ProductList.jsx';\nimport Sidebar from 