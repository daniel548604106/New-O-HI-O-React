ta } = await apiGetBanners();\n      console.log('banners', data);\n      setBanners(data.banners);\n    };\n    getBanners();\n  }, []);\n\n  //取得 Products\n  useEffect(() => {\n    const getAllProducts = async () => {\n      try {\n        const { data } = await apiGetAllProducts();\n        setProducts(data.products);\n        console.log(data);\n      } catch (error) {\n        console.log(error);\n      }\n    };\n    getAllProducts();\n  }, []);\n  return (\n    <div>\n      <Banner banners={banners} />\n      <main>\n        <section>\n          <NewItems products={products} t={t} />\n        </section>\n        <section>\n          <PopularItems products={products} t={t} />\n        </section>\n        <div className={classes.campaign}>\n          <Campaign products={products} t={t} />\n        </div>\n        <section>\n          <Shop t={t} />\n        </section>\n        <section>\n          <DiscountedItems products={products} t={t} />\n        </section>\n        <section style={{ width: '100%' }}>\n          <Subscription t={t} />\n        </section>\n      </main>\n    </div>\n  );\n};\n\nexport default Home;\n","/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/components/StripeButton/StripeButton.jsx",[],"/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/pages/Products/_Product/_Product.jsx",["361","362","363","364","365","366","367","368"],"/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/store/reducerTypes.js",[],"/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/store/cart/cartAction.jsx",[],"/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/api/authRequest.js",[],"/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/api/productRequest.js",[],"/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/api/userRequest.js",[],"/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/api/bannerRequest.js",[],"/Users/daniel548604106/Desktop/react-shoe-ecommerce/clie