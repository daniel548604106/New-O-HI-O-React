ata } = await apiGetUserData(userId);\n        console.log('res', data);\n        dispatch(setUserLoggedIn(data.user));\n      } catch (error) {\n        console.log(error);\n      }\n    };\n    if (query) {\n      userId = query.split('=')[1];\n      getUserData();\n    }\n  }, []);\n\n  useEffect(() => {\n    setOpen(isLoginModalShow);\n  }, [isLoginModalShow]);\n\n  return (\n    <Router history={history}>\n      <Navbar />\n      {isLoginModalShow}\n      <div className=\"loginModal\">\n        <Backdrop open={open} onClick={handleClose} style={{ zIndex: 11 }}>\n          <div\n            onClick={preventProp}\n            style={{\n              maxWidth: '600px',\n\n              borderRadius: '10px',\n              zIndex: 12,\n              backgroundColor: '#fff',\n            }}\n          >\n            <form noValidate autoComplete=\"off\">\n              <LoginModal />\n            </form>\n          </div>\n        </Backdrop>\n      </div>\n      <main>\n        <Route path=\"/oauth/:type\" exact>\n          <OAuth props={props} />\n        </Route>\n        <Route path=\"/shoes\" exact>\n          <Shoes />\n        </Route>\n        <Route path=\"/account\" exact>\n          <Account />\n        </Route>\n        <Route path={`/products/:id`}>\n          <Product />\n        </Route>\n        <Route path=\"/checkout\" exact>\n          <Checkout />\n        </Route>\n        <Route path=\"/cart\" exact>\n          <Cart />\n        </Route>\n        <Route path=\"/beauty\">\n          <Beauty />\n        </Route>\n        <Route path=\"/\" exact>\n          <Home />\n        </Route>\n      </main>\n      <Footer />\n    </Router>\n  );\n};\n\nexport default App;\n","/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/store/store.js",[],"/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/pages/Cart/Cart.jsx",["211","212","213"],"import React, { useState } from 'react';\nimport './Cart.scss';\nimport { Divider } from '@materi