o[name] = value;\n    setCheckoutDetail({\n      ...checkoutDetail,\n      personalInfo: { ...personalInfo },\n    });\n  };\n  useEffect(() => {\n    console.log(personalInfo);\n  }, []);\n\n  if (checkoutDetail && personalInfo) {\n    return (\n      <>\n        <p className={classes.cardTitle}>訂購人資訊</p>\n        <div className={classes.cardLayout}>\n          <div className={classes.cardBody}>\n            <div className={`${classes.name} ${classes.row}`}>\n              <p>姓名</p>\n              <input\n                onChange={(e) => handleCheckoutDetail(e)}\n                value={checkoutDetail.personalInfo.name}\n                name=\"name\"\n                type=\"text\"\n              />\n            </div>\n            <div className={`${classes.tel} ${classes.row}`}>\n              <p>手機</p>\n              <input\n                value={personalInfo.phone}\n                onChange={(e) => handleCheckoutDetail(e)}\n                name=\"phone\"\n                type=\"numeric\"\n                maxLength=\"10\"\n              />\n            </div>\n            <div className={`${classes.email} ${classes.row}`}>\n              <p>Email</p>\n              <input\n                value={personalInfo.email}\n                onChange={(e) => handleCheckoutDetail(e)}\n                name=\"email\"\n                type=\"text\"\n              />\n            </div>\n          </div>\n        </div>\n      </>\n    );\n  }\n};\n\nCardPaymentOrdererInfoCard.propTypes = {\n  setCheckoutDetail: PropTypes.func,\n  checkoutDetail: PropTypes.object,\n};\n\nexport default CardPaymentOrdererInfoCard;\n","/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/components/Cart/CartPaymentInfo/InvoiceInfoCard/InvoiceInfoCard.jsx",["758"],"import React, { useState } from 'react';\nimport classes from './InvoiceInfoCard.module.scss';\nimport UnifiedInvoiceTab from './UnifiedInvoiceTab/UnifiedInvoiceTab.jsx';\nimport DonateInvoiceTab from './DonateInvoiceTab/Donat