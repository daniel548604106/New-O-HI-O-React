?ixid=MXwxMjA3fDB8MHxzZWFyY2h8NHx8ZmFzaGlvbnxlbnwwfHwwfA%3D%3D&ixlib=rb-1.2.1&auto=format&fit=crop&w=600&q=60',\n  },\n  {\n    label: 'Bali, Indonesia',\n    imgPath:\n      'https://images.unsplash.com/photo-1487222477894-8943e31ef7b2?ixid=MXwxMjA3fDB8MHxzZWFyY2h8MTV8fGZhc2hpb258ZW58MHx8MHw%3D&ixlib=rb-1.2.1&auto=format&fit=crop&w=600&q=60',\n  },\n  {\n    label: 'NeONBRAND Digital Marketing, Las Vegas, United States',\n    imgPath:\n      'https://images.unsplash.com/photo-1506152983158-b4a74a01c721?ixid=MXwxMjA3fDB8MHxzZWFyY2h8N3x8ZmFzaGlvbnxlbnwwfHwwfA%3D%3D&ixlib=rb-1.2.1&auto=format&fit=crop&w=600&q=60',\n  },\n  {\n    label: 'Goč, Serbia',\n    imgPath:\n      'https://images.unsplash.com/photo-1485125639709-a60c3a500bf1?ixid=MXwxMjA3fDB8MHxzZWFyY2h8MTl8fGZhc2hpb258ZW58MHx8MHw%3D&ixlib=rb-1.2.1&auto=format&fit=crop&w=600&q=60',\n  },\n];\n\nconst Banner = () => {\n  return (\n    <div>\n      <Swiper\n        id=\"swiper-banner\"\n        spaceBetween={10}\n        slidesPerView={1}\n        navigation\n        autoPlay\n        AutoPlaySwipeableViews\n        pagination={{ clickable: true }}\n        onSwiper={(swiper) => console.log(swiper)}\n        onSlideChange={() => console.log('slide change')}\n      >\n        {banners.map((banner) => (\n          <SwiperSlide key={banner.label}>\n            <div\n              className={classes.swiperBackground}\n              style={{ backgroundImage: `url(${banner.imgPath})` }}\n            ></div>\n          </SwiperSlide>\n        ))}\n      </Swiper>\n    </div>\n  );\n};\n\nexport default Banner;\n","/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/components/Home/Shops/Shops.jsx",[],"/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/components/Home/Subscription/Subscription.jsx",[],"/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/components/Home/Campaign/Campaign.jsx",[],"/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/components/Home/NewItems/NewI