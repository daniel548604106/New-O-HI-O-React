     if (window.pageYOffset > 400) {\n        setShowBanner(true);\n      } else {\n        setShowBanner(false);\n      }\n    });\n    return () => {\n      window.removeEventListener('scroll', () => {\n        if (window.pageYOffset > 400) {\n          console.log('hihihi');\n        }\n      });\n    };\n  }, []);\n  // Fetch Reviews\n\n  useEffect(() => {\n    const getReviews = async () => {\n      const { data } = await apiGetReviews(params.id);\n      console.log('reviews=>', data);\n      setReviews(data.reviews);\n    };\n    getReviews();\n  }, []);\n\n  // Fetch Product\n\n  useEffect(() => {\n    const getProduct = async () => {\n      const { data } = await apiGetProduct(params.id);\n      console.log(data.product);\n      setProduct(data.product);\n    };\n    getProduct();\n  }, []);\n\n  // Fetch Recommended Products\n  useEffect(() => {\n    const getRecommendedProducts = async () => {\n      const { data } = await apiGetAllProducts();\n      console.log('recommended =>', data);\n      setRecommendedProducts(data.products);\n    };\n    getRecommendedProducts();\n  }, []);\n\n  const scrollToPage = (idx) => {\n    idx === 0\n      ? window.scrollTo({ behavior: 'smooth', top: productDescription.current.offsetTop - 80 })\n      : window.scrollTo({ behavior: 'smooth', top: evaluation.current.offsetTop - 80 });\n  };\n\n  return (\n    <div className={classes.productRoot}>\n      {/* <div className=\"notification\">\n        <Notification title=\"已加入慾望清單\" />\n      </div> */}\n      <div className={showBanner ? classes.showBanner : classes.hideBanner}>\n        <ProductBanner product={product} scrollToPage={scrollToPage} />\n      </div>\n      <div className={classes.containerLayout}>\n        <div ref={topDisplay} className={classes.productDisplay}>\n          <ProductDisplay product={product} />\n        </div>\n        <div className={classes.productMainInfo}>\n          <div className={classes.info}>\n            <ProductInfo t={t} product={product} />\n          </div>\n          <div className={classes.cta}>\n            <ProductCTA product={product} />\n          </div>\n        </div>\n      </div>\n      <div className={classes.containerLayout}>\n        <div className={classes.productDescription}>\n          <ProductDescription\n            t={t}\n            productDescriptionRef={productDescription}\n            evaluationRef={evaluation}\n            id=\"product-description\"\n            product={product}\n            reviews={reviews}\n          />\n        </div>\n        <div className={classes.designShopInfo}>\n          <DesignShopInfo />\n        </div>\n      </div>\n      <div>\n        <ProductRecommendation products={recommendedProducts} />\n      </div>\n    </div>\n  );\n};\n\nexport default Product;\n","/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/store/reducerTypes.js",[],"/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/store/cart/cartAction.jsx",[],"/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/api/authRequest.js",[],"/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/api/productRequest.js",[],"/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/api/userRequest.js",[],"/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/api/bannerRequest.js",[],"/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/components/Login/Login.js",[],"/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/store/index/indexReducer.js",[],"/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/components/Latest/MobileFilter/MobileFilter.jsx",[],"/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/components/Global/Tags/Tags.jsx",[],"/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/store/product/productReducer.jsx",[],"/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/components/Home/Shops/Shops.jsx",[],"/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/