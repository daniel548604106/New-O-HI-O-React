 = 'product';\n    const { data } = apiAddToFavorite(params.id, type);\n    console.log(data);\n  };\n\n  return (\n    <div className={classes.bannerRoot}>\n      <div className={classes.bannerLayout}>\n        <div className={classes.scrollBtnRow}>\n          {scrollBtns.map((scrollBtn, idx) => (\n            <div key={scrollBtn.name}>\n              <a onClick={() => scrollEffect(idx)} className={activeBtn === idx && classes.active}>\n                {scrollBtn.name}\n              </a>\n            </div>\n          ))}\n        </div>\n        <div className={classes.ctaBtnRow}>\n          <button onClick={() => addToWishList()} className={classes.wishList}>\n            <FavoriteBorderIcon />\n            <p>Add To WishList</p>\n          </button>\n          <button onClick={() => addItemToCart(product)} className={classes.addToCart}>\n            <p>Add To Cart</p>\n          </button>\n        </div>\n      </div>\n    </div>\n  );\n};\n\nProductBanner.propTypes = {\n  product: PropTypes.object,\n  scrollToPage: PropTypes.func,\n};\n\nexport default ProductBanner;\n","/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/components/Product/ProductDisplay/ProductDisplay.jsx",[],"/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/components/Product/ProductDescription/ProductDescription.jsx",["513","514"],"import React, { useRef } from 'react';\nimport Dropdown from '../../Global/Dropdown/Dropdown.jsx';\nimport PropTypes from 'prop-types';\nimport Stars from '../../Global/Stars/Stars.jsx';\nimport classes from './ProductDescription.module.scss';\nconst ProductDescription = ({ product, evaluationRef, productDescriptionRef, t, reviews }) => {\n  const productDescription = useRef(null);\n  const evaluation = useRef(null);\n  return (\n    <div>\n      <div ref={productDescriptionRef} className=\"description\">\n        <Dropdown title=\"productDescription\" product={product} />\n      </div>\n      <div className=\"description\">\n        <Dropdown title=\"tags\" product={product} />\n      </div>\n      <div ref={evaluationRef}>\n        <h2>{t('evaluation')}</h2>\n        <hr />\n        <div className={classes.feedbackRow}>\n          <div>\n            {reviews ? (\n              reviews.map((review) => (\n                <div key={review._id} className={classes.feedbackRow}>\n                  <img src={review.user.picture} alt=\"avatar\" />\n                  <div>\n                    <div className={classes.topRow}>\n                      <p className={classes.name}>\n                        <span>{review.user.name}</span> 在一週前所留下的評價\n                      </p>\n                      <div className={classes.starRow}>\n                        <Stars score={review.score} />\n                      </div>\n                    </div>\n                    <p>{review.comment.text}</p>\n                    {review.comment.images.map((image) => (\n                      <img className={classes.commentImage} key={image} src={image} alt=\"\" />\n                    ))}\n                  </div>\n                </div>\n              ))\n            ) : (\n              <h1>No reviews</h1>\n            )}\n          </div>\n        </div>\n        <hr />\n      </div>\n    </div>\n  );\n};\n\nProductDescription.propTypes = {\n  product: PropTypes.object,\n  evaluationRef: PropTypes.func,\n  productDescriptionRef: PropTypes.func,\n  reviews: PropTypes.array,\n  t: PropTypes.func,\n};\n\nexport default ProductDescription;\n","/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/lib/oAuth.js",[],"/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/components/Global/Stars/Stars.jsx",["515","516"],"import React, { useEffect, useState } from 'react';\nimport Star from '../../../assets/images/global/star.png';\nimport StarGrey from '../../../assets/images/global/star-grey.png';\nimport classes from './Stars.module.scss';\nimport PropTypes from 'prop-types';\nconst Stars = ({ score }) => {\n  const [starArr, setStarArr] = useState([]);\n  useEffect(() => {\n    let integer = Number((score + '').split('.')[0]);\n    let decimal = Number((score + '').split('.')[1]);\n    const count = () => {\n      for (let i = 0; i < integer; i++) {\n        starArr.push(1);\n      }\n      if (isNaN(decimal) || decimal === undefined || 0) return;\n      console.log(score, integer, decimal);\n      starArr.push(+`0.${decimal}`);\n      console.log(starArr);\n    };\n    count();\n  }, []);\n  return (\n    <div className={classes.starRow}>\n      {starArr.map((star, idx) => (\n        <li\n          key={idx}\n          style={{\n            width: '30px',\n            height: '30px',\n            display: 'block',\n            backgroundImage: `url(${StarGrey})`,\n            position: 'relative',\n          }}\n        >\n          <span\n            style={{\n              backgroundImage: `url(${Star})`,\n              width: '100%',\n              height: '100%',\n              display: 'block',\n              position: 'absolute',\n              top: '0',\n              left: '0',\n            }}\n          ></span>\n        </li>\n      ))}\n    </div>\n  );\n};\n\nStars.propTypes = {\n  score: PropTypes.number,\n};\n\nexport default Stars;\n","/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/components/Global/Dropdown/Dropdown.jsx",[],"/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/components/Global/ShopCard/ShopCard.jsx",[],"/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/components/Product/ProductInfo/ProductInfo.jsx",[],"/Users/daniel548604106/Desktop/react-shoe-ecommerce/client/src/components/Product/ProductCTA/ProductCTA.jsx",["517"],"import React from 'react';\nimport classes from './ProductCTA.module.scss';\nimport FavoriteIcon from '@material-ui/icons/Favorite';\nimport ChevronRightIcon from '@material-ui/icons/ChevronRight';\nimport PropTypes from 'prop-types';\nimport { useParams } from 'react-router-dom';\nimp