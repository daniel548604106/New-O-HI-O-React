{
  "name": "O-HI-O-server",
  "version": "1.0.0",
  "engines": {
    "node": "14.15.0"
  },
  "scripts": {
    "client": "cd client && npm start",
    "server": "cd server && nodemon server.js",
    "build": "cd client && npm run build",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\" ",
    "start": "cd server && node server.js",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false cd client && npm install && npm run build"
  },
  "devDependencies": {
    "nodemon": "^2.0.7"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "js-cookie": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "mongoose": "^5.11.12",
    "multer": "^1.4.2",
    "node-sass": "^4.14.1",
    "qs": "^6.9.6",
    "query-string": "^6.13.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "socket.io": "^3.1.1"
  }
}
